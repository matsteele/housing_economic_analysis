{
    "presets": [
        ["@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ], 
        "@babel/typescript", "@babel/react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-syntax-dynamic-import"
        ]
    ]
}